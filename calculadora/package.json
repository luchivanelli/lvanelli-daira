{
    "name": "astric",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "cli": "node ./.astric/cli/app.js",
        "theme": "smui-theme compile FrontEnd/src/base/theme.css -i FrontEnd/ASTRIC/",
        "b": "air ",
        "mig": "go run main.go --modo=Migratio",
        "f": "vite --mode front --open --config .astric/vite/vite.config.ts",
        "dev": "vite --mode full --open --config .astric/vite/vite.config.ts",
        "login": "vite --mode login --open --config .astric/vite/vite.config.ts",
        "build": "vite build --force --config .astric/vite/vite.config.ts",
        "preview": "vite preview --config .astric/vite/vite.config.ts",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "prod": "vite --config .astric/vite/vite.prod.config.ts"
    },
    "devDependencies": {
        "@material/typography": "^14.0.0",
        "@skeletonlabs/skeleton": "^1.11.0",
        "@sveltejs/vite-plugin-svelte": "^2.4.2",
        "@tsconfig/svelte": "^3.0.0",
        "autoprefixer": "^10.4.14",
        "postcss": "^8.4.24",
        "postcss-load-config": "^4.0.1",
        "svelte": "^3.55.1",
        "svelte-check": "^2.10.3",
        "tailwindcss": "^3.3.2",
        "tslib": "^2.5.0",
        "typescript": "^4.9.3",
        "vite": "^4.1.0"
    },
    "dependencies": {
        "@sveltejs/adapter-auto": "^2.0.0",
        "axios": "^1.6.7",
        "chart.js": "^3.9.1",
        "crypto": "^1.0.1",
        "eslint": "^8.23.1",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-svelte3": "^4.0.0",
        "fs-extra": "^10.1.0",
        "mathjs": "^11.2.1",
        "moment": "^2.29.4",
        "prettier": "^2.7.1",
        "prettier-plugin-svelte": "^2.7.0",
        "svelte-chartjs": "^3.0.1",
        "svelte-dts": "^0.3.7",
        "svelte-forms-lib": "^2.0.1",
        "svelte-spa-router": "^3.3.0",
        "tailwind": "^4.0.0",
        "v8n": "^1.5.1",
        "vite-plugin-child-process": "^1.0.6",
        "vite-tsconfig-paths": "^4.0.5",
        "yup": "^0.32.11"
    }
}
